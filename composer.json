{
    "name": "cego/filebeat-logger-laravel",
    "description": "Filebeat logging for Laravel",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "cego/filebeat-logger": "^2.0.6",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/contracts": "^11.0|^12.0",
        "ext-json": "*",
        "illuminate/http": "^11.0",
        "matomo/device-detector": "^6.2",
        "ext-apcu": "*"
    },
    "require-dev": {
        "cego/php-cs-fixer": "2.0.0",
        "cego/phpstan": "^4",
        "laravel/framework": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Cego\\": "src/"
        }
    },
    "autoload-dev": {},
    "extra": {
        "laravel": {
            "providers": [
                "Cego\\FilebeatLogging\\FilebeatLoggingServiceProvider"
            ]
        }
    }
}
